{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "plugins": ["plugin:prettier/recommended"],
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": "warn",
    "no-eval": "error"
  }
}
